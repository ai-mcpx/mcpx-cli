{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "io.modelcontextprotocol.anonymous/test-server-python",
  "title": "Test Python MCP Server",
  "description": "A test mcp server in python",
  "websiteUrl": "https://anonymous.modelcontextprotocol.io/test-server-python",
  "repository": {
    "url": "https://github.com/example/test-server-python",
    "source": "github",
    "id": "example/test-server-python"
  },
  "version": "1.0.0",
  "packages": [
    {
      "registryType": "pypi",
      "registryBaseUrl": "https://pypi.org",
      "identifier": "test-server-python",
      "version": "1.0.0",
      "runtimeHint": "python",
      "transport": {
        "type": "stdio"
      },
      "runtimeArguments": [
        {
          "type": "positional",
          "valueHint": "config_path",
          "description": "Path to configuration file",
          "format": "filepath",
          "default": "./config.json",
          "isRequired": true
        }
      ],
      "environmentVariables": [
        {
          "name": "MCP_HOST",
          "description": "Server host address",
          "format": "string",
          "isRequired": false,
          "default": "0.0.0.0"
        },
        {
          "name": "MCP_PORT",
          "description": "Server port number",
          "format": "number",
          "isRequired": false,
          "default": "8002"
        }
      ]
    },
    {
      "registryType": "docker",
      "registryBaseUrl": "https://docker.io",
      "identifier": "example/test-server-python",
      "version": "1.0.0",
      "runtimeHint": "docker",
      "transport": {
        "type": "stdio"
      },
      "runtimeArguments": [
        {
          "type": "named",
          "name": "--rm",
          "description": "Remove container after exit",
          "isRequired": true
        },
        {
          "type": "named",
          "name": "-p",
          "valueHint": "port_mapping",
          "description": "Port mapping for the container",
          "format": "string",
          "default": "8002:8000",
          "isRequired": false
        }
      ],
      "environmentVariables": [
        {
          "name": "MCP_HOST",
          "description": "Server host address",
          "format": "string",
          "isRequired": false,
          "default": "0.0.0.0"
        },
        {
          "name": "MCP_PORT",
          "description": "Server port number",
          "format": "number",
          "isRequired": false,
          "default": "8002"
        }
      ]
    }
  ],
  "remotes": [
    {
      "type": "streamable-http",
      "url": "https://anonymous.modelcontextprotocol.io/test-server-python",
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json",
          "description": "Content type for JSON requests"
        }
      ]
    }
  ]
}
