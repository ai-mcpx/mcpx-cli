{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "io.modelcontextprotocol.anonymous/test-server-binary",
  "title": "Test Binary MCP Server",
  "description": "A test mcp server with binary distribution",
  "websiteUrl": "https://anonymous.modelcontextprotocol.io/test-server-binary",
  "repository": {
    "url": "https://github.com/example/test-server-binary",
    "source": "github",
    "id": "example/test-server-binary"
  },
  "version": "1.0.0",
  "packages": [
    {
      "registryType": "binary",
      "registryBaseUrl": "https://github.com",
      "identifier": "https://github.com/example/test-server-binary/releases/download/v1.0.0/test-server-binary-linux-x64",
      "version": "1.0.0",
      "runtimeHint": "binary",
      "transport": {
        "type": "stdio"
      },
      "runtimeArguments": [
        {
          "type": "named",
          "name": "--port",
          "valueHint": "port_number",
          "description": "Server port number",
          "format": "number",
          "default": "8000",
          "isRequired": false
        },
        {
          "type": "named",
          "name": "--host",
          "valueHint": "host_address",
          "description": "Server host address",
          "format": "string",
          "default": "0.0.0.0",
          "isRequired": false
        },
        {
          "type": "named",
          "name": "--config",
          "valueHint": "config_path",
          "description": "Path to configuration file",
          "format": "filepath",
          "default": "./config.json",
          "isRequired": false
        }
      ],
      "environmentVariables": [
        {
          "name": "MCP_LOG_LEVEL",
          "description": "Logging level",
          "format": "string",
          "isRequired": false,
          "default": "INFO"
        },
        {
          "name": "MCP_DATA_DIR",
          "description": "Data directory path",
          "format": "string",
          "isRequired": false,
          "default": "./data"
        }
      ]
    },
    {
      "registryType": "docker",
      "registryBaseUrl": "https://docker.io",
      "identifier": "example/test-server-binary",
      "version": "1.0.0",
      "runtimeHint": "docker",
      "transport": {
        "type": "stdio"
      },
      "runtimeArguments": [
        {
          "type": "named",
          "name": "--rm",
          "description": "Remove container after exit",
          "isRequired": true
        },
        {
          "type": "named",
          "name": "-p",
          "valueHint": "port_mapping",
          "description": "Port mapping for the container",
          "format": "string",
          "default": "8000:8000",
          "isRequired": false
        }
      ],
      "environmentVariables": [
        {
          "name": "MCP_LOG_LEVEL",
          "description": "Logging level",
          "format": "string",
          "isRequired": false,
          "default": "INFO"
        },
        {
          "name": "MCP_DATA_DIR",
          "description": "Data directory path",
          "format": "string",
          "isRequired": false,
          "default": "/app/data"
        }
      ]
    }
  ],
  "remotes": [
    {
      "type": "streamable-http",
      "url": "https://anonymous.modelcontextprotocol.io/test-server-binary",
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json",
          "description": "Content type for JSON requests"
        }
      ]
    }
  ]
}
