{
  "name": "io.modelcontextprotocol.anonymous/test-server-docker",
  "description": "A test mcp server with docker distribution",
  "repository": {
    "url": "https://github.com/example/test-server-docker",
    "source": "github",
    "id": "example/test-server-docker"
  },
  "version": "1.0.0",
  "packages": [
    {
      "registryType": "docker",
      "registryBaseUrl": "https://docker.io",
      "identifier": "example/test-server-docker",
      "version": "1.0.0",
      "runtimeHint": "docker",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "name": "MCP_LOG_LEVEL",
          "description": "Logging level",
          "format": "string",
          "isRequired": false,
          "default": "INFO"
        },
        {
          "name": "MCP_DATA_DIR",
          "description": "Data directory path",
          "format": "string",
          "isRequired": false,
          "default": "/app/data"
        }
      ],
      "runtimeArguments": [
        {
          "name": "--port",
          "description": "Server port number",
          "type": "number",
          "isRequired": false,
          "default": "8005",
          "valueHint": "port_number"
        },
        {
          "name": "--host",
          "description": "Server host address",
          "type": "string",
          "isRequired": false,
          "default": "0.0.0.0",
          "valueHint": "host_address"
        },
        {
          "name": "--config",
          "description": "Path to configuration file",
          "type": "string",
          "isRequired": false,
          "default": "./config.json",
          "valueHint": "config_path"
        }
      ]
    }
  ],
  "remotes": [
    {
      "type": "streamable-http",
      "url": "https://anonymous.modelcontextprotocol.io/test-server-docker",
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json",
          "description": "Content type for JSON requests"
        }
      ]
    }
  ]
}
