{
  "name": "io.modelcontextprotocol.anonymous/test-server-wheel",
  "description": "A test mcp server with wheel package distribution",
  "status": "active",
  "repository": {
    "url": "https://github.com/example/test-server-wheel",
    "source": "github",
    "id": "example/test-server-wheel"
  },
  "version": "1.0.0",
  "packages": [
    {
      "registry_type": "wheel",
      "identifier": "https://artifactory.com/example/test-server-wheel/releases/download/v1.0.0/test_server_wheel-1.0.0-py3-none-any.whl",
      "version": "1.0.0",
      "runtime_hint": "python",
      "transport": {
        "type": "stdio"
      },
      "runtime_arguments": [
        {
          "type": "string",
          "name": "config_path",
          "value_hint": "config_path",
          "description": "Path to configuration file",
          "default": "./config.json",
          "is_required": true
        }
      ],
      "environment_variables": [
        {
          "name": "MCP_HOST",
          "description": "Server host address",
          "format": "string",
          "is_required": false,
          "default": "0.0.0.0"
        },
        {
          "name": "MCP_PORT",
          "description": "Server port number",
          "format": "number",
          "is_required": false,
          "default": "8003"
        }
      ]
    },
    {
      "registry_type": "docker",
      "identifier": "example/test-server-wheel",
      "version": "1.0.0",
      "runtime_hint": "docker",
      "transport": {
        "type": "stdio"
      },
      "runtime_arguments": [
        {
          "type": "named",
          "name": "--rm",
          "value_hint": "--rm",
          "description": "Remove container after exit",
          "default": "",
          "is_required": true
        },
        {
          "type": "string",
          "name": "-p",
          "value_hint": "port_mapping",
          "description": "Port mapping for the container",
          "default": "8003:8000",
          "is_required": false
        }
      ],
      "environment_variables": [
        {
          "name": "MCP_HOST",
          "description": "Server host address",
          "format": "string",
          "is_required": false,
          "default": "0.0.0.0"
        },
        {
          "name": "MCP_PORT",
          "description": "Server port number",
          "format": "number",
          "is_required": false,
          "default": "8003"
        }
      ]
    }
  ],
  "remotes": [
    {
      "type": "streamable-http",
      "url": "https://anonymous.modelcontextprotocol.io/test-server-wheel",
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json",
          "description": "Content type for JSON requests"
        }
      ]
    }
  ]
}
