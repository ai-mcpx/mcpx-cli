  "status": "active",
  "repository": {
    "url": "https://github.com/example/test-server",
    "source": "github",
    "id": "example/test-server"
  },
  "version_detail": {
    "version": "1.0.0",
    "release_date": "2025-07-20T12:00:00Z",
